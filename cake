import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();  
        int M=sc.nextInt();  
        int cost=N*30;
        int revenue=M*50;
        int money=revenue-cost;
        System.out.println(money);
	}
}
