import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);  
	    int X=sc.nextInt(); 
	    int Y=sc.nextInt(); 
	    int small=X*30; 
	    int large=Y*60; 
	    System.out.println(small+large);
	}
}
