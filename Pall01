import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
			Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int a=sc.nextInt();
		    int sum=0;
		    int n=a;
		    while(a>0)
		    {
		        int r=a%10;
		        sum=(sum*10)+r;
		        a=a/10;
		    }
		    if(n==sum)
		    System.out.println("wins");
		    else
		    System.out.println("loses");
		}

	}
}
