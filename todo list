import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int c;
	    for(int i=0;i<n;i++){
	        c=0;
	        int a=sc.nextInt();
	        int b[]=new int[a];
	        for(int j=0;j<a;j++){
	            b[j]=sc.nextInt();
	        }
	        for(int j=0;j<a;j++){
	            if(b[j]>=1000)
	            c++;
	        }
	        System.out.println(c);
	    }

	}
}
