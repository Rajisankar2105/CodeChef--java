import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int[]a=new int[4];
		for(int i=0;i<4;i++){
		    a[i]=sc.nextInt();
		}
		int Count=0;
		for(int i=0;i<4;i++){
		    if(a[i]>=10){
		        Count++;
		    }
		}
		System.out.println(Count);
	}
}
