import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
	    int N=sc.nextInt();
	    if(N<=5)
	    {
	        System.out.println(N*200);
	    }
	    else{
	        System.out.println("1000");
	    }
	}
}
